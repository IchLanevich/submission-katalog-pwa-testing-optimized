{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "eol-last": 0,
        "no-console": 0,
        "no-undef": 0,
        "import/prefer-default-export": 0,
        "no-return-assign": 0,
        "max-len": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "prefer-arrow-callback": 0,
        "no-prototype-builtins": 0,
        "func-names": 0,
        "eqeqeq": 0,
        "import/no-extraneous-dependencies": 0,
        "no-restricted-globals": 0
    }
}
